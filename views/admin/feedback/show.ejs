<div>
    <div class="row">
        <div class="col-lg-12">
            <ol class="breadcrumb">
                <li><a href="#">&nbsp;</a></li>
            </ol>

            <div class="clearfix">
                <h1 class="pull-left"><a href="/admin/feedback">Сообщения</a></h1>
            </div>
        </div>
    </div>


    <div class="row">
        <div class="col-lg-12">
            <div class="main-box no-header clearfix">
                <div class="main-box-body clearfix">
                    <h2>Сообщение от <%= feedback.name%></h2>
                    <p>Email: <%= feedback.email%></p>
                    <blockquote>
                        <p>
                            <%= feedback.message%>
                        </p>
                        <small>
                            <cite title=""><%= feedback.subject%></cite>
                        </small>
                    </blockquote>

                </div>
            </div>
        </div>
    </div>
</div>



