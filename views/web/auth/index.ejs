<div class="min-style">
  <div class="content-wrapper clearfix">
    <div class="row">
      <div class="col full">

        <h1>Административная часть</h1>
        <form method="post" action="/happsyadmin/login">
          <input type="hidden" name="_csrf" value="<%= _csrf %>" />
          <div class="form-item">
            <input class="form-input" type="text" name="username" placeholder="Имя прользователя">
          </div>

          <div class="form-item">
            <input class="form-input" type="password" name="password" placeholder="Пароль">
          </div>

          <input class="form-button" type="submit" value="ВОЙТИ">
        </form>

      </div>
    </div>
  </div>
</div>
